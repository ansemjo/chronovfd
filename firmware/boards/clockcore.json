{
  "COMMENT": "copied from esp32dev board definition",
  "build": {
    "arduino":{
      "ldscript": "esp32_out.ld"
    },
    "core": "esp32",
    "extra_flags": "-DARDUINO_ESP32_DEV",
    "f_cpu": "240000000L",
    "f_flash": "40000000L",
    "flash_mode": "dio",
    "mcu": "esp32",
    "variant": "esp32"
  },
  "connectivity": [
    "wifi",
    "bluetooth",
    "ethernet",
    "can"
  ],
  "debug": {
    "openocd_board": "esp-wroom-32.cfg"
  },
  "frameworks": [
    "arduino",
    "espidf"
  ],
  "name": "ChronoVFD ESP32 ClockCore Module",
  "upload": {
    "flash_size": "8MB",
    "maximum_ram_size": 532480,
    "maximum_size": 8388608,
    "require_upload_port": true,
    "speed": 2000000
  },
  "url": "https://github.com/ansemjo/chronovfd",
  "vendor": "Custom"
}
